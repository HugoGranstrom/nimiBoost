{
  "scopeName": "python-markdown.injection",
  "fileTypes": ["nim"],
  "injectionSelector": "L:source.nim",
  "patterns": [
    {
      "patterns": [
        {
          "include": "#markdown-in-python"
        }
      ]
    }
  ],
  "repository": {
    "markdown-in-python": {
      "name": "meta.embedded.block.markdown",
      "begin": "(nbText:) (\"\"\")",
      "beginCaptures": {
        "1": {
          "name": "source.nim"
        },
        "2": {
          "name": "string.quoted.triple.nim"
        }
      },
      "end": "(\"\"\")",
      "endCaptures": {
        "1": {
          "name": "string.quoted.triple.nim"
        }
      },
      "patterns": [{ "include": "text.html.markdown" }]
    }
  }
}
