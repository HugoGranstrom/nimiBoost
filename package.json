{
  "name": "python-string-markdown",
  "displayName": "python-string-markdown",
  "description": "highlight python strings as Markdown",
  "version": "0.1.0",
  "publisher": "nukopy",
  "icon": "",
  "engines": {
    "vscode": "^1.45.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "python",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nukopy/python-string-markdown"
  },
  "license": "MIT",
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/highlight-markdown-string.json",
        "scopeName": "python-markdown.injection",
        "injectTo": [
          "source.nim"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.markdown": "markdown"
        }
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/python.json"
      }
    ]
  }
}
